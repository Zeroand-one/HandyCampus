.conent {
  background-color: #eee;
}

.conent .pan {
  padding: 10rpx !important;
  margin: 30rpx !important;
  border-radius: 20rpx;
  background-color: #fff;
}

.conent .pan .item {
  height: 64rpx;
  line-height: 64rpx;
  padding-left: 10rpx;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #333;
}

.conent .pan .bz {
  height: 300rpx;
  color: #333;
  font-size: 30rpx;
  padding-left: 10rpx;
}

.conent .order .te {
  font-size: 36rpx;
  padding-left: 50rpx;
}

.conent .order .te text {
  color: #ff7300;
}

.conent .img {
  font-size: 30rpx;
  color: #333;
}

.conent .img .itemImg {
  width: 200rpx;
  height: 200rpx;
  margin: 10rpx 10rpx;
}

.conent .btn {
  width: 60%;
  margin: 10rpx auto;
}

.conent .btn button {
  padding: 0 !important;
  line-height: 60rpx !important;
  height: 60rpx !important;
  border-radius: 30rpx !important;
  font-size: 32rpx;
}